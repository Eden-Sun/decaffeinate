{
  "name": "decaffeinate",
  "version": "1.41.3",
  "description": "Move your CoffeeScript source to JavaScript using ES6 syntax.",
  "main": "dist/decaffeinate.cjs.js",
  "jsnext:main": "dist/decaffeinate.es6.js",
  "bin": {
    "decaffeinate": "./bin/decaffeinate"
  },
  "scripts": {
    "lint": "eslint src test",
    "pretest": "rollup -c rollup.test.config.js",
    "test": "mocha build/test-bundle.js",
    "prebuild": "npm run lint",
    "build": "rollup -c rollup.cjs.config.js && rollup -c rollup.es6.config.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eventualbuddha/decaffeinate.git"
  },
  "keywords": [
    "coffee",
    "coffee-script",
    "convert",
    "es3",
    "es6"
  ],
  "author": "Brian Donovan",
  "license": "MIT",
  "dependencies": {
    "coffee-script-redux": "https://github.com/michaelficarra/CoffeeScriptRedux.git",
    "detect-indent": "^4.0.0",
    "magic-string": "^0.8.0",
    "repeating": "^2.0.0"
  },
  "files": [
    "bin",
    "dist"
  ],
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-eslint": "^5.0.0-beta4",
    "babel-plugin-syntax-flow": "^6.3.13",
    "babel-plugin-transform-flow-strip-types": "^6.3.15",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-es2015-rollup": "^1.0.0",
    "browserify": "^12.0.1",
    "eslint": "^1.2.1",
    "eslint-plugin-babel": "^3.0.0",
    "mocha": "^2.2.5",
    "mversion": "^1.10.1",
    "rollup": "^0.21.1",
    "rollup-plugin-babel": "^2.2.0",
    "rollup-plugin-multi-entry": "^1.0.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
